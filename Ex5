import java.util.Scanner ;
public class lab {
    public static void main (String[] args)
    {
        int x = 100, y = 200;
        //Before swap
        System.out.println("x = " + x);
        System.out.println("y = " + y);
        int temp = x;
        x = y;
        y = temp;
        //After swap
        System.out.println("x = " + x);
        System.out.println("y = " + y);
    }

    }
